Call Main()
Sub Main()
	Dim strLayer, arrLayers
	
	Rhino.EnableRedraw(False)
	
	arrLayers = Rhino.LayerNames
	For Each strLayer In arrLayers
		Rhino.LayerLocked strLayer, False
	Next
	
	Rhino.EnableRedraw(true)
End Sub